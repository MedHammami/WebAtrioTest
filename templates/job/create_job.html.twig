{# templates/user/add_job.html.twig #}

{% extends 'base.html.twig' %}

{% block title %}Ajouter un emploi{% endblock %}

{% block body %}
    <h1>Ajouter un emploi</h1>

    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.position_occupied) }}
    {{ form_row(form.user) }}

    <button type="submit" class="btn btn-primary">Enregistrer</button>
    {{ form_end(form) }}
{% endblock %}
